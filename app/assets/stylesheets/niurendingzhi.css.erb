@charset "utf-8";
/* CSS Document */
/* 首页 */

.main{height:100%;width:100%; overflow: hidden;}
.lbt{ height:34.7%; width:100%; overflow:hidden;}
.lbt img{ width:100%;}
.logo{ position:absolute; z-index:1; bottom:3.5%; left:0; width:100%; text-align:center;}
.banner{ width:80%; position:relative; height:40%; overflow:hidden; margin:5% auto;}
.banner a{display: block;
    position: absolute;
	z-index:1;
    width: 33%;
	height:20%;
    top: 0;
    overflow: hidden;
    -webkit-transition: .6s opacity;
    transition: .6s opacity;
    color: #fff;
	line-height:1em;
    text-transform: uppercase;
    text-shadow: 0 0 5px rgba(0, 0, 0, 0.4);
	text-align:center; font-size:1.6rem; font-weight:bolder; padding:11% 0 2em 0; background: url(<%= image_url 'bg123.png' %>) no-repeat; background-size:100%;}
.banner a img{ width:50%;}
a.chanpin { margin-top: 3.2%; left: 34.4%;}
a.mendian { margin-top: 22%; left: 15%;}
a.jinjia { left: 54%; margin-top: 22%;}
a.gongsi{left: 34.4%; margin-top: 41%;}

/* 产品 */
.header{background:-webkit-linear-gradient(top,#dedede,#fff); line-height:2.4em; font-size:2.1rem; color:#a9884a; overflow:hidden; border-bottom:1px solid #b0afb0; border-top:1px solid #a9a6a5; position:fixed; z-index:1; top:0; left:0; width:100%;}
.header a, .header span{ width:33.33%; float:left; text-align:center; font-weight:bolder;}
.header span{ background:#a98749; color:#fff;}
.cplb{ width:100%; left:0; position:fixed; z-index:1; top:50%; margin-top:-100px;}
.cplb a{ display:block; width:47.96%; text-align:center; margin:3% auto; line-height:3em; font-size:1.7rem; font-weight:bolder; color:#a9884a; position:relative; }
.cplb a img{ position:absolute; z-index:1; left:0 ;top:0; width:100%; height:100%;}
.search{background:-webkit-linear-gradient(top,#dedede,#fff);border:1px solid #c0c1c1; width:80%; margin:2% auto; padding: 0 0 0 10px; border-radius:1px;}
.search .neirong{ border:none; background:none; line-height:60px; font-size:1.6rem; width:80%; font-weight:bolder; }
.search .sou{ background:url(<%= image_url 'search.png' %>) no-repeat center; width:20%; border:none; height:60px;}

.chanpinliebiao{ width:100%; padding: 5% 0 80px 9%; overflow:hidden;}
.chanpinliebiao ul{ width:24%; margin: 0 4.5% 0 0; float:left;line-height:1em;}
.chanpinliebiao ul li, .chanpinliebiao ul li img{text-align:center; width:100%;line-height:1em;}
.chanpinliebiao ul li.title{padding:5px 0 20px 0;  }
.chanpinliebiao ul li a{ color:#a98749; font-size:1.3rem; font-weight:bolder; line-height:1em;}
.main2{ padding:62px 0 0 0;}

.prdh{ position:fixed; color:#9f9c94; text-align:center; z-index:1; bottom:0; left:0; width:100%; background:#a98749; line-height:60px; font-size:1.5rem;  }
.prdh a{ color:#fff; font-weight:bolder;}
.prdh .select{ color:#582d15;}

.cptp{ width:59.375%; margin:5% auto;}
.cptp img{ width:100%;}
.cpcs{ width:70%; margin:0 auto; color:#a98749; line-height:1.1em; font-size:1.7rem;}
.cpcs td{ padding:0 8px 8px 0;}
.cpcs .title{ color:#999;}
.cpjs{ width:80%; color:#a98749; margin:5% auto;font-size:1.7rem;}

.jjtitle{ width:87.5%; margin:0 auto; padding:30px 0 10px 0;}
.jjtitle .jjico{ border:2px solid #a98749; background:rgba(100,100,100,0.2); padding:4px 4px 2px 4px; margin: 0 20px 0 0;}
.g_stlye { background: #bc955a; height: 20px; width: 90%; margin: 0 auto; border-radius: 20px;}
.g_stlye2{ background:#cecfce;}
.jzjj{ width:87.5%; margin:0 auto; text-align:center; background:#fde8ce;}
.jzjj td{ border-bottom:1px solid #ceb07d; font-size:1.7rem; line-height:1.85em; }
td.last{ border:none;}
.jzjj2{ background:#e5e5e4;}

.chanpinliebiao2 ul li img{ width:15.38%;}
.chanpinliebiao2 ul{ background:#c1a35b; margin-bottom:15px; border-radius:10px;}
.chanpinliebiao2 ul li a{ color:#fff; display:block; padding:2em 0; line-height:1.6rem;}

.tanchufuceng{ position:fixed; width:100%; height:100%; z-index:99; top:0; left:0; background:rgba(0,0,0,0.6);}
.tanchufuceng ul{ width:80%; margin:30% auto; background:rgba(255,255,255,0.8); border-radius:10px; padding:10% 0;text-align:center; color:#3d1407; position:relative;}
.tanchufuceng ul li a{ color:#3d1407; font-size:1.7rem;line-height:2.2em; display:block; }
.tanchufuceng ul li.title{ font-size:3rem; padding: 0 0 15px; }
.closed{ position:absolute; z-index:1; right:10px; top:10px; background:url(<%= image_url 'closed.png' %>); display:block; width:26px; height:26px; background-size:100%;}
.mdlb{ width:70%; padding: 5%; background:#eee; margin:5% auto; border-radius:5px; }
.mdlb b{ font-size:1.7rem; color:#a98749; font-weight:bolder;}
.mdlb div{font-size:1.5rem; padding:5px 0; overflow:hidden; line-height:37px; width:100%;}
.mdlb img{ margin: 0 10px 0 0 ; float:left;}
.mdlb .xxdz{ line-height:1.1em; padding:8px 0 0 45px; width:80%; background:url(<%= image_url 'weizhi2.png' %>) no-repeat 0 8px; min-height:37px; }

.wzxq{ padding:10% 0; width:87%; line-height:1.5em; margin:0 auto;}
.wzxq p{ padding: 0 0 1em; font-size:1.5rem; color:#333; line-height:1.5em;}

.cptp2{ padding-top:15%;}
.cptp3{ width:90%;}
.cptp3 .biaoti{ display:block; width:94%; background:#eee; margin:8px 0 0 0; font-size:1.5rem; font-weight:bolder; border-radius:1px; padding:8px 3%;}

.wzxq2{ padding:5% 0;}
.time{ color:#a98749; font-size:1.4rem;}
.wzxq2 p{ color:#a98749;}
.wzxq2 h2{ font-weight:bolder; font-size:1.5rem;color:#a98749; padding: 0 0 1em;}

.lxfs{ width:90%; margin:20% auto 10%; font-size:1.5rem;}
.lxfs td{ padding:0 0 1em 0; color:#333; }
.lxfs .title{ text-align:right; min-width:60px;}

.erweima { margin:0 auto; width:90%; overflow:hidden; padding: 0 0 10%;}
.erweima p{ width:45%; margin: 0 2.5%; float:left; text-align:center; font-size:1.5rem;}
.erweima p img{ width:100%;}